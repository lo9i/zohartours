  <nav class="site-navbar navbar navbar-default navbar-fixed-top navbar-mega" role="navigation">
    <div class="navbar-header">
      <button type="button" class="navbar-toggle hamburger hamburger-close navbar-toggle-left hided"
      data-toggle="menubar">
        <span class="sr-only">Toggle navigation</span>
        <span class="hamburger-bar"></span>
      </button>
      <button type="button" class="navbar-toggle collapsed" data-target="#site-navbar-collapse"
      data-toggle="collapse">
        <i class="icon wb-more-horizontal" aria-hidden="true"></i>
      </button>
      <div class="navbar-brand navbar-brand-center site-gridmenu-toggle" data-toggle="gridmenu">
        <!-- <img class="navbar-brand-logo" src="../assets/images/logo.png" title="Remark"> -->
        <span class="navbar-brand-text"> {{ brand }}</span>
      </div>
    </div>
    <div class="navbar-container container-fluid">
      <!-- Navbar Collapse -->
      <div class="collapse navbar-collapse navbar-collapse-toolbar" id="site-navbar-collapse">
        <!-- Navbar Toolbar -->
        <ul class="nav navbar-toolbar">
          <li class="hidden-float" id="toggleMenubar">
            <a data-toggle="menubar" href="#" role="button">
              <i class="icon hamburger hamburger-arrow-left">
                  <span class="sr-only">Toggle menubar</span>
                  <span class="hamburger-bar"></span>
                </i>
            </a>
          </li>
          <li class="hidden-xs">
            <a href="{{ path('home') }}" target="_blank" role="button">
              <i class="icon fa-desktop"></i> {{ 'backend.visitSite'|trans }}
            </a>
          </li>
        </ul>
        <!-- End Navbar Toolbar -->
        <!-- Navbar Toolbar Right -->
        <ul class="nav navbar-toolbar navbar-right navbar-toolbar-right">
          <li class="dropdown">
            {% set route = app.request.get('_route') %}
            {% set params = app.request.get('_route_params') %}
            {% set locale = app.request.locale %}
            {% if  params['_locale'] == 'en' %}
              {% set params = params|merge({'_locale': 'es'}) %}
              <a class="dropdown-toggle" data-toggle="dropdown" href="javascript:void(0)" data-animation="scale-up"
              aria-expanded="false" role="button">
                <span class="flag-icon flag-icon-us"></span>
              </a>
              <ul class="dropdown-menu" role="menu">
                <li role="presentation">
                  <a href="{{ path(route, params ) }}" role="menuitem">
                    <span class="flag-icon flag-icon-ar"></span> {{ 'backend.spanish'|trans({}, "messages", 'es') }}</a>
                </li>
              </ul>
            {% else %}
              {% set params = params|merge({'_locale': 'en'}) %}
              <a class="dropdown-toggle" data-toggle="dropdown" href="javascript:void(0)" data-animation="scale-up"
              aria-expanded="false" role="button">
                <span class="flag-icon flag-icon-ar"></span>
              </a>
              <ul class="dropdown-menu" role="menu">
                <li role="presentation">
                  <a href="{{ path(route, params ) }}" role="menuitem">
                    <span class="flag-icon flag-icon-us"></span> {{ 'backend.english'|trans({}, "messages", 'en') }}</a>
                </li>
              </ul>
            {% endif %}
          </li>
          <li class="dropdown">
            <a class="navbar-avatar dropdown-toggle" data-toggle="dropdown" href="#" aria-expanded="false"
            data-animation="scale-up" role="button">
              <span class="avatar avatar-online">
                <img src="{{ asset('img/avatars/default.png') }}" alt="{{ app.user }}">
                <i></i>
              </span>
              {{ app.user }} <i class="icon wb-chevron-down-mini" aria-hidden="true"></i>
            </a>
            <ul class="dropdown-menu" role="menu">
              <li role="presentation">
                <a href="{{ path('fos_user_security_logout') }}" role="menuitem"><i class="icon wb-power" aria-hidden="true"></i> {{ 'backend.logout'|trans }}</a>
              </li>
            </ul>
          </li>
        </ul>
        <!-- End Navbar Toolbar Right -->
      </div>
      <!-- End Navbar Collapse -->
    </div>
