{% extends 'GushhFrontendBundle::base.html.twig' %}

{% block title %}
    {{ brand }} | {{ entity|title }}
{% endblock %}

{% block body %}
    <div class="container">
      <div class="row">
        <div class="col-sm-12">
          <h2 class="Title">{{ entity }} <span>-</span></h2>
        </div>
      </div>
      <div class="row">
        <div class="col-sm-12">
          <div class="StaticPage-content">
            {{ entity.content|raw }}          
          </div>
        </div>
      </div>
    </div>
{% endblock %}
