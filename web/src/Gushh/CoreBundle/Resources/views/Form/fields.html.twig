{% block gender_widget %}
    {% spaceless %}
        {% if expanded %}
            <ul {{ block('widget_container_attributes') }}>
            {% for child in form %}
                <li>
                    {{ form_widget(child) }}
                    {{ form_label(child) }}
                </li>
            {% endfor %}
            </ul>
        {% else %}
            {# just let the choice widget render the select tag #}
            asd
            {{ block('choice_widget') }}
        {% endif %}
    {% endspaceless %}
{% endblock %}

{# form_div_layout.html.twig #}
{% block html5time_widget %}
    {% set type = type|default('time') %}
    {{ block('form_widget_simple') }}
{% endblock html5time_widget %}

{% block html5date_widget %}
    {% set type = type|default('date') %}
    {{ block('form_widget_simple') }}
{% endblock html5date_widget %}

{% block html5color_widget %}
    {% set type = type|default('color') %}
    {{ block('form_widget_simple') }}
{% endblock html5color_widget %}
